{
  "name": "vui-button",
  "version": "1.0.3",
  "description": "Mixins and CSS for applying button styles",
  "scripts": {
    "clean": "rimraf *.css",
    "prebuild": "npm run clean",
    "build:autoprefix": "postcss --use autoprefixer button.css -o button.css",
    "build": "node-sass --output-style expanded button.css.scss button.css && npm run build:autoprefix",
    "postinstall": "bower install",
    "pretest": "npm run build",
    "test": "csslint button.css && karma start karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/valence-ui-button.git"
  },
  "keywords": [
    "d2l",
    "valence-ui",
    "vui"
  ],
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "dependencies": {
    "bower": "^1.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^5.2.0",
    "csslint": "^0.10.0",
    "jasmine-dom-matchers": "^0.1.0",
    "karma": "^0.12.32",
    "karma-cli": "0.0.4",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "node-sass": "^3.1.1",
    "postcss-cli": "^1.3.1",
    "rimraf": "^2.3.4"
  }
}
