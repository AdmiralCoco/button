{
  "description": "Polymer-based web components and Sass mixins for D2L buttons",
  "repository": {
    "type": "git",
    "url": "https://github.com/BrightspaceUI/button.git"
  },
  "name": "d2l-button",
  "scripts": {
    "build:sass": "node-sass --output-style expanded ./test/acceptance/button.css.scss ./test/acceptance/button.css",
    "predump": "rimraf test/acceptance/dumps",
    "dump": "concurrently -p name -n serve,galen -s first -k \"polymer serve --npm\" \"npm run galen:local:dump\"",
    "galen:local:run": "d2l-galen test test/acceptance/button.test.js -g factory:local -s",
    "galen:local:dump": "d2l-galen dump test/acceptance/button.test.js -d test/acceptance/dumps -g factory:local -s",
    "galen:sauce:run": "sc-run -- d2l-galen test test/acceptance/button.test.js -g factory:sauce -s -i SAUCE_USERNAME SAUCE_ACCESS_KEY TRAVIS_REPO_SLUG TRAVIS_BUILD_NUMBER -- -p4",
    "lint": "npm run lint:wc && npm run lint:html",
    "lint:html": "eslint *.js *.html demo/*.html test/*.html test/acceptance/*.html test/acceptance/*.js",
    "lint:wc": "polymer lint --npm *.js",
    "pretest:galen:local": "npm run build:sass",
    "test:galen:local": "concurrently -p name -n serve,galen -s first -k \"polymer serve --npm\" \"npm run galen:local:run\"",
    "pretest:galen:sauce": "npm run build:sass",
    "test:galen:sauce": "concurrently -p name -n serve,galen -s first -k \"polymer serve --npm\" \"npm run galen:sauce:run\"",
    "test:polymer:local": "polymer test --npm --skip-plugin sauce",
    "test:polymer:sauce": "polymer test --npm --skip-plugin local",
    "test": "npm run lint && npm run test:polymer:local && npm run test:galen:local"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polymer/iron-component-page": "^3.0.0-pre.18",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.18",
    "@polymer/iron-test-helpers": "^3.0.0-pre.18",
    "@webcomponents/webcomponentsjs": "^2.0.2",
    "babel-eslint": "^8.2.5",
    "chromedriver": "^2.30.1",
    "concurrently": "^3.5.0",
    "d2l-galen-utils": "git+https://github.com/Brightspace/d2l-galen-utils.git#v0.3.0",
    "eslint": "^4.15.0",
    "eslint-config-brightspace": "^0.4.0",
    "eslint-plugin-html": "^4.0.1",
    "galenframework": "^2.3.4",
    "node-sass": "^4.5.2",
    "rimraf": "^2.6.1",
    "sauce-connect-launcher": "^1.2.4",
    "sauceconnect-runner": "git+https://github.com/Brightspace/sauceconnect-runner.git#v0.3.0"
  },
  "version": "5.0.0",
  "resolutions": {
    "inherits": "2.0.3",
    "samsam": "1.1.3",
    "supports-color": "3.1.2",
    "type-detect": "1.0.0",
    "@webcomponents/webcomponentsjs": "2.0.2",
    "chalk": "2.4.1",
    "ansi-styles": "3.2.1",
    "has-flag": "3.0.0",
    "debug": "3.1.0",
    "source-map": "0.5.7",
    "del": "3.0.0",
    "request": "2.87.0",
    "globby": "6.1.0",
    "pify": "3.0.0",
    "glob": "7.1.2",
    "once": "1.4.0",
    "minimist": "1.2.0",
    "aws-sign2": "0.7.0",
    "form-data": "2.3.2",
    "har-validator": "5.0.3",
    "http-signature": "1.2.0",
    "performance-now": "2.1.0",
    "qs": "6.5.2",
    "ajv": "5.5.2",
    "har-schema": "2.0.0",
    "assert-plus": "1.0.0",
    "extsprintf": "1.4.0",
    "commander": "2.16.0",
    "read-pkg": "3.0.0",
    "load-json-file": "4.0.0",
    "path-type": "3.0.0",
    "parse-json": "4.0.0",
    "strip-bom": "3.0.0",
    "semver": "5.5.0",
    "yargs": "7.1.0",
    "camelcase": "3.0.0",
    "string-width": "2.1.1",
    "yargs-parser": "5.0.0",
    "strip-ansi": "4.0.0",
    "ansi-regex": "3.0.0",
    "is-fullwidth-code-point": "2.0.0",
    "cross-spawn": "5.1.0",
    "progress": "2.0.0",
    "which": "1.3.1",
    "acorn": "5.7.1",
